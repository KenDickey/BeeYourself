!NumberParser2 methodsFor: 'private' stamp: 'KenD 11/22/2023 07:41:11'!
nextIntegerRadix: radix
	| value valid digit |
	value := 0.
	valid := false.
	[
		digit := self nextDigit: radix.
		digit isNil]
		whileFalse: [
			valid := true.
			value := value * radix + digit].
