	(memRef baseRegister = addressReg)
		ifFalse: [ self move: memRef baseRegister to: addressReg ].
	(memRef offset isZero)
		ifFalse: [ self addImm: memRef offset to: addressReg ].
	(memRef indexRegister isZero)
