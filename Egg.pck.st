	| prefix |
	legacy := #() writeStream.
	[
		prefix := LegacyPrefix withValue: aStream peek.
		prefix isNil]
		whileFalse: [
			legacy nextPut: prefix.
			contents nextPut: aStream next]
! !

!ISAPrefixReader methodsFor: 'private' stamp: 'KenD 11/22/2023 13:48:34'!
