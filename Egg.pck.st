		name: #_basicAt:.
	patch := OTernaryOperation
		receiver: callsite
		left: 0
		right: nativeCode
		name: #_basicAt:put:.
	self activationRecord addUse: callsite at: #left.
	callsite addUse: patch at: #receiver.
	nativeCode addUse: patch at: #right.
	instruction dissolveReplacingWith: patch.
	patch beforeInsert: callsite
