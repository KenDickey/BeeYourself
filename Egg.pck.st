	| uninitialized |
	uninitialized := Set new: temporaries.
	1 to: temporaries do: [:i | uninitialized add: -1 - i].
	aBasicBlock instructionsDo: [:inst | | base index |
		inst isMessageSend ifTrue: [^uninitialized].
		inst isStore ifTrue: [
